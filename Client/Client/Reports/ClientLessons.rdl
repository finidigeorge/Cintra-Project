  <DataSets>
    <DataSet Name="Data">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <CommandText>SELECT date_on, begin_time, end_time, h.nickname as horse, c.name as coach
FROM bookings b,  coaches c, horses h
WHERE  b.is_deleted = 0 and b.horse_id = h.id and b.coach_id = c.id and b.client_id = @ClientId and b.date_on between @StartDate and @EndDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="ClientId">
            <Value>={?ClientId}</Value>
          </QueryParameter>
          <QueryParameter Name="StartDate">
            <Value>=Convert.ToDateTime({?StartDate})</Value>
          </QueryParameter>
          <QueryParameter Name="EndDate">
            <Value>=Convert.ToDateTime({?EndDate})</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
      <Fields>
        <Field Name="date_on">
          <DataField>date_on</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="begin_time">
          <DataField>begin_time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="end_time">
          <DataField>end_time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="horse">
          <DataField>horse</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="coach">
          <DataField>coach</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
